    "babel-loader": "^8.0.4",
    "css-loader": "^0.28.10",
    "semantic-release": "^15.10.5",
    "source-map-loader": "^0.2.4",
    "standard": "^11.0.1",
    "style-loader": "^0.20.3",
    "travis-deploy-once": "^5.0.9",
